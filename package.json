    "clsx": "^2.1.1",
    "express": "^4.18.2",
    "lucide-react": "^0.462.0",
    "openai": "^5.9.0",
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "react-hook-form": "^7.51.0",
